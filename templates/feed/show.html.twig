{% extends 'base.html.twig' %}

{% block title %}Feed{% endblock %}

{% block body %}
    <h1>Feed</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feed.id }}</td>
            </tr>
            <tr>
                <th>FeedUrl</th>
                <td>{{ feed.feedUrl }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ feed.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ feed.description }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ feed.icon }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('feed_index') }}">back to list</a>

    <a href="{{ path('feed_edit', {'id': feed.id}) }}">edit</a>

    {{ include('feed/_delete_form.html.twig') }}
{% endblock %}
